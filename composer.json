{
    "name": "elazar/auryn-configuration",
    "description": "Modularized configurations for the Auryn dependency injector",
    "license": "MIT",
	"support": {
        "email": "me@matthewturland.com",
        "issues": "https://github.com/elazar/auryn-configuration/issues",
        "source": "https://github.com/elazar/auryn-configuration"
    },
    "authors": [
        {
            "name": "Matthew Turland",
            "homepage": "http://matthewturland.com"
        }
    ],
    "keywords": [
        "dependency injection",
        "configuration",
        "Auryn"
    ],
    "require": {
        "rdlowrey/auryn": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Elazar\\Auryn\\Configuration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elazar\\Auryn\\Configuration\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
